<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="it.eng.spagobi.profiling.bean.SbiUserHistory" table="SBI_USER_HISTORY">

		<id name="eventId" type="int">
			<column name="EVENT_ID" />
			<generator class="org.hibernate.id.enhanced.TableGenerator">
				<param name="optimizer">none</param>
				<param name="segment_value">SBI_USER_HISTORY</param>
				<param name="initial_value">1</param>
			</generator>
		</id>

		<component name="commonInfo" insert="true" update="true"
			class="it.eng.spagobi.commons.metadata.SbiCommonInfo">
			<property name="userIn" type="string">
				<column name="USER_IN" length="100" not-null="false" />
			</property>
			<property name="userUp" type="string">
				<column name="USER_UP" length="100" not-null="false" />
			</property>
			<property name="userDe" type="string">
				<column name="USER_DE" length="100" not-null="false" />
			</property>
			<property name="timeIn" type="timestamp">
				<column name="TIME_IN" not-null="false" />
			</property>
			<property name="timeUp" type="timestamp">
				<column name="TIME_UP" not-null="false" />
			</property>
			<property name="timeDe" type="timestamp">
				<column name="TIME_DE" not-null="false" />
			</property>
			<property name="sbiVersionIn" type="string">
				<column name="SBI_VERSION_IN" length="10" not-null="false" />
			</property>
			<property name="sbiVersionUp" type="string">
				<column name="SBI_VERSION_UP" length="10" not-null="false" />
			</property>
			<property name="sbiVersionDe" type="string">
				<column name="SBI_VERSION_DE" length="10" not-null="false" />
			</property>
			<property name="organization" type="string">
				<column name="ORGANIZATION" length="20" not-null="false" />
			</property>
		</component>

		<property name="id" type="int">
			<column name="ID" not-null="true" />
		</property>
		<property name="userId" type="string">
			<column name="USER_ID" length="100" not-null="true" />
		</property>
		<property name="password" type="string">
			<column name="PASSWORD" length="150" />
		</property>
		<property name="fullName" type="string">
			<column name="FULL_NAME" />
		</property>
		<property name="dtPwdBegin" type="timestamp">
			<column name="DT_PWD_BEGIN" length="19" />
		</property>
		<property name="dtPwdEnd" type="timestamp">
			<column name="DT_PWD_END" length="19" />
		</property>
		<property name="flgPwdBlocked" type="java.lang.Boolean">
			<column name="FLG_PWD_BLOCKED" />
		</property>
		<property name="dtLastAccess" type="timestamp">
			<column name="DT_LAST_ACCESS" length="19" />
		</property>
		<property name="isSuperadmin" type="java.lang.Boolean">
			<column name="IS_SUPERADMIN" />
		</property>

		<property name="timeStart" type="timestamp">
			<column name="TIME_START" length="19" />
		</property>
		<property name="timeEnd" type="timestamp">
			<column name="TIME_END" length="19" />
		</property>

		<filter name="tenantFilter" condition="organization=:tenant" />
	</class>
	<filter-def name="tenantFilter">
		<filter-param name="tenant" type="string" />
	</filter-def>
</hibernate-mapping>
